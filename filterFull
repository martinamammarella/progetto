package utils;
import java.util.ArrayList;
import java.util.Date;
import java.util.Vector;
import model.Lavori;
import org.json.simple.*;
public class filterFull extends Filters {
	public filterFull(String[] city, String[] linguaggi,String remoto, String link, Date data,String oreLavoro){
		 super(city,linguaggi,remoto,link,data,oreLavoro);
	}
 public ArrayList<Lavori> filtraggio(String[] fcity,Vector<Lavori> offerte) {
	 ArrayList<Lavori> filtrato = new ArrayList<Lavori>();
	 for(Lavori z:offerte) {
		 
			 if(z.getOrarioLavoro()!=null) {
			filtrato.add(z);
				 
		 }
	 
	
 }
	return filtrato;
}
 }
