package utils;

import java.util.Date;
import java.util.Vector;

import org.json.simple.JSONArray;

import model.Lavori;

public class filterLin extends Filters {
	public filterLin(String[] city, String[] linguaggi,String remoto, String link, Date data,String oreLavoro){
		 super(city,linguaggi,remoto,link,data,oreLavoro);
	}
	public JSONArray filtraggio(String[] flin,Vector<Lavori> offerte) {public JSONArray filtraggio(String[] flin,Vector<Lavori> offerte) {
		 JSONArray a= new JSONArray();
		 for(Lavori z:offerte) {
			 List <String> l=z.getLinguaggi();
			 
			  {
			 for(String c:l) {
				 for(String c1:flin) {
					if(c.equals(c1)){
						  a.add(z);
					 }
					 {
						 
				 
			 
				 }
					 
			 }}
		 }
		 
	 }
		 return a;

}}
